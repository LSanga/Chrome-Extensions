{
	"manifest_version": 2,

	"name": "test-disable-drag-drop",
	"description": "Disable drag and drop from Chrome",
	"version": "1.0",
	"icons": {
		"128": "icon.png"
	},

    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["thirdParty/jquery-3.6.0.min.js", "background.js"]
        }
      ],

	"permissions": [
		"activeTab"
	]
}